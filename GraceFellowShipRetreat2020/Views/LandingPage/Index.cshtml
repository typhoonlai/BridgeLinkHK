<body data-spy="scroll" data-target="#main-menu">

    <!--Start Page loader -->
    <div id="pageloader">
        <div class="loader">
            <img src="/img/landing/progress.gif" alt='loader' />
        </div>
    </div>
    <!--End Page loader -->
    <!--Start Slider -->
    <section id="home">
        <div class="slider-overlay"></div>
        <div class="homeslider flexslider">
            <ul class="slides">
                <li class="slide1 bg"></li>
                <li class="slide2 bg"></li>
                <li class="slide3 bg"></li>
            </ul>
        </div><!--/ homeslider-->
        <!-- start text slider-->
        <div class="tb">
            <div class="tb-cell">
                <div class="slide-text">
                    <ul id="text-slider">
                        <li><h1>有移民煩惱?</h1></li>
                        <li><h1>博翹國際</h1></li>
                        <li><h1>有移民上的問題?</h1></li>
                    </ul>
                    <p>我們很高興為您服務</p>
                    <div class="page-scroll scroll">
                        <a class="btn btn-lg btn-primary btn-color-white" style="border-radius:5px;" href="/home/index">移民到新加坡</a>
                        <a class="btn btn-lg btn-primary btn-color-white" style="border-radius:5px;" href="http://www.briglinkca.com">移民加拿大</a>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!--End Slider-->
    <div>
    </div>
    <a href="#" class="scrollup"> <i class="fa fa-chevron-up"> </i> </a>
    <!--Plugins-->
    <script type="text/javascript" src="/js/landing/jquery-1.11.1.min.js"></script>
    <script type="text/javascript" src="/js/landing/bootstrap.min.js"></script>
    <script type="text/javascript" src="/js/landing/owl-carousel/owl.carousel.js"></script>
    <script type="text/javascript" src="/js/landing/jquery.flexslider-min.js"></script>
    <script type="text/javascript" src="/js/landing/jquery.bxslider.min.js"></script>
    <script type="text/javascript" src="/js/landing/jquery.magnific-popup.min.js"></script>
    <script type="text/javascript" src="/js/landing/easing.js"></script>
    <script type="text/javascript" src="/js/landing/jquery.parallax-1.1.3.js"></script>
    <script type="text/javascript" src="/js/landing/jquery.sticky.js"></script>
    <script type="text/javascript" src="/js/landing/jquery.mixitup.min.js"></script>
    <script type="text/javascript" src="/js/landing/wow.min.js"></script>
    <script type="text/javascript" src="/js/landing/custom.js"></script>
</body>
</html>
